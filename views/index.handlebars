<p>SERVER APPEARS TO BE CONNECTED</p>


<div class="not-devoured">
    <ul>
        {{#each burger_data}}
            {{#unless this.devoured}}

            <li>
                {{this.burger_name}}
                <form method="POST" action="/burgers/update">
                <input value = {{this.id}} type="hidden" name="id">
                <button type="submit" data-id = "{{this.id}}">Devour!</button>
                </form>
            </li>

            {{/unless}}
        {{/each}}
            
    </ul>

</div>


<div class="devoured">
    <ul>

    </ul>

</div>